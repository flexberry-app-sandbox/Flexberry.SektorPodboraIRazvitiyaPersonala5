<h3 class="ui header">{{t "forms.i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e.анкеты-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.анкеты
      displayAttributeName="номер"
      autocomplete=true
      relationName="анкеты"
      projection="АнкетыL"
      title=(t "forms.i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e.анкеты-caption")
      readonly=readonly
      componentName="анкетыLookup"
      class=(if (v-get validationModel "анкеты" "isInvalid") "error")
      data-test-i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e-анкеты=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "анкеты" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e.анкеты.датаЗаполн-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=true
      value=model.анкеты.датаЗаполн
      class=(if (v-get validationModel.анкеты "датаЗаполн" "isInvalid") "error")
      data-test-i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e-анкеты-датаЗаполн=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.анкеты "датаЗаполн" "message")}}
  </div>
  <div class="field" data-test-i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e-анкеты-главнПричин="true">
    <label>{{t "forms.i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e.анкеты.главнПричин-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-sektor-podbora-i-razvitiya-personala-факторы")
      value=model.анкеты.главнПричин
      class=(if (v-get validationModel.анкеты "главнПричин" "isInvalid") "error")
      readonly=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.анкеты "главнПричин" "message")}}
  </div>
  <div class="field" data-test-i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e-анкеты-полУволСотр="true">
    <label>{{t "forms.i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e.анкеты.полУволСотр-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-sektor-podbora-i-razvitiya-personala-пол")
      value=model.анкеты.полУволСотр
      class=(if (v-get validationModel.анкеты "полУволСотр" "isInvalid") "error")
      readonly=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.анкеты "полУволСотр" "message")}}
  </div>
  <div class="field" data-test-i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e-анкеты-относКатСотр="true">
    <label>{{t "forms.i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e.анкеты.относКатСотр-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-sektor-podbora-i-razvitiya-personala-катег-сотруд")
      value=model.анкеты.относКатСотр
      class=(if (v-get validationModel.анкеты "относКатСотр" "isInvalid") "error")
      readonly=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.анкеты "относКатСотр" "message")}}
  </div>
  <div class="field" data-test-i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e-анкеты-возрастСотруд="true">
    <label>{{t "forms.i-i-s-sektor-podbora-i-razvitiya-personala-инф-из-анкет-e.анкеты.возрастСотруд-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-sektor-podbora-i-razvitiya-personala-возраст")
      value=model.анкеты.возрастСотруд
      class=(if (v-get validationModel.анкеты "возрастСотруд" "isInvalid") "error")
      readonly=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel.анкеты "возрастСотруд" "message")}}
  </div>
</form>
